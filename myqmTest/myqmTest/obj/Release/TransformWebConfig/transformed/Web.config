    <sessionState mode="InProc" timeout="20"/>
    <!--<httpHandlers>
      <add type="AustinHarris.JsonRpc.Handlers.AspNet.JsonRpcHandler" verb="*" path="*.rpc"/>
    </httpHandlers>-->
    <webServices>
      <protocols>
        <add name="HttpPost"/>
        <add name="HttpGet"/>
      </protocols>
    </webServices>
    <customErrors mode="Off"/>
  </system.web>
  <system.webServer>
